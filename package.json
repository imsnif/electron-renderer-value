    "babel-tape-runner": "^2.0.1",
    "electron": "^1.4.2",
    "electron-rebuild": "^1.2.1",
    "istanbul": "^0.4.5",
    "nyc": "^8.3.0",
    "proxyquire": "^1.7.10",
    "spectron": "^3.4.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2"
